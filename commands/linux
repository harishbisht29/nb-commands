
# enable autocomplete command in zsh
echo 'autoload -Uz compinit && compinit' >> ~/.zshrc
source ~/.zshrc

# find a text in all the fileI::

# scp command to remove on a port
scp -i /home/co/sys/gateway.pem -P 8802 test gateway@16.170.218.73:/home/gateway/rsync/nats_tls_cert/
# -p ⇒"Preserves modification times, access times, and modes from the original file." -r ⇒ "Recursively copy entire directories." -q ⇒ "Disables the progress meter.
scp -prq server1:dir1/. server2:dir2/

# list file sie in mb
ls -l --block-size=M

# list file created data
ls -lc

# list all the cron jobs
for user in $(cut -f1 -d: /etc/passwd); do crontab -u $user -l; done

# read env from custom env file
export $(grep -v '^#' my_custom.env | xargs)

nmap -Pn3000-4000 mysqlserver2.telldus.orahi.com


# wget remote file without ssl cert check
